import turtle
t=turtle.Turtle()
s=turtle.Screen()
t.color("white")
t.fillcolor("red")
t.begin_fill()
t.forward(80)
t.left(90)
t.forward(52)
t.left(90)
t.forward(80)
t.left(90)
t.forward(52)
t.end_fill()
t.penup()
t.goto(34,34)
t.pendown()
t.fillcolor("white")
t.begin_fill()
t.forward(20)
t.left(120)
t.forward(20)
t.left(120)
t.forward(20)
t.end_fill()
t.hideturtle()
